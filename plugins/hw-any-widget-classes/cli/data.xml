            <wp:ping_status>closed</wp:ping_status>
            <wp:status>publish</wp:status>
            <wp:post_parent>0</wp:post_parent>
            <wp:menu_order>0</wp:menu_order>
            <wp:post_type>hw_mysidebar</wp:post_type>
            <wp:post_password></wp:post_password>
            <wp:is_sticky>0</wp:is_sticky>
            <wp:postmeta>
                <wp:meta_key>_edit_last</wp:meta_key>
                <wp:meta_value><![CDATA[1]]></wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>enable</wp:meta_key>
                <wp:meta_value><![CDATA[0]]></wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>query_data_and</wp:meta_key>
                <wp:meta_value>
                    <hw:params>
                        <params>
                            <params>
                                <param name="act"><![CDATA[post_types]]></param>
                                <param name="compare"><![CDATA[==]]></param>
                                <param name="act_values"><![CDATA[page]]></param>
