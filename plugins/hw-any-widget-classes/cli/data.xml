                                <param name="act"><![CDATA[templates]]></param>
                                <param name="compare"><![CDATA[==]]></param>
                                <param name="act_values"><![CDATA[single]]></param>
                            </params>
                        </params>
                    </hw:params>
                </wp:meta_value>

            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>query_data_or</wp:meta_key>
                <wp:meta_value>
                    <hw:params>
                        <params>
                            <params>
                                <param name="act"><![CDATA[-1]]></param>
                                <param name="compare"><![CDATA[==]]></param>
                            </params>
                        </params>
                    </hw:params>
                </wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>sidebar_1</wp:meta_key>
                <wp:meta_value><![CDATA[sidebar-1]]></wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
