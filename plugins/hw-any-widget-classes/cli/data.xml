        <option>
            <params name="HW_Sidebar_Settings" prefix="sidebar_2_" method="append">
                <param name="enable_override_sidebar" type="bool"><![CDATA[0]]></param>
                <param name="alphabe_widgets" type="bool">1</param>
            </params>
        </option>
        <option>
            <params name="HW_Module_Settings_page" prefix="hw_any_widget_classes_" method="append">
                <params name="sidebars_pos">
                    <params name="sidebars_position">
                        <params name="sidebar-1">
                            <param name="position"><![CDATA[after_header]]></param>
                        </params>
                        <params name="sidebar-2">
                            <param name="position"><![CDATA[before_footer]]></param>
                        </params>
                    </params>

                </params>

            </params>
        </option>
    </options>
