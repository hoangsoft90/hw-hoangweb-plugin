            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>widgets_for_shortcodes</wp:meta_key>
                <wp:meta_value><![CDATA[]]></wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>hwskin_footers_default_footer_col1</wp:meta_key>
                <wp:meta_value><![CDATA[]]></wp:meta_value>
            </wp:postmeta>
            <wp:postmeta>
                <wp:meta_key>hw_sidebar_data</wp:meta_key>
                <wp:meta_value><![CDATA[]]></wp:meta_value>
            </wp:postmeta>
        </item>
    </posts>
    <skins>
        <hw:skin name="awc">
            <skin:apply_plugin>hw-any-widget-classes</skin:apply_plugin>
            <skin:default_skin_path>skins</skin:default_skin_path>
            <skin:skin_type>file</skin:skin_type>
            <skin:skin_name>hwawc-skin.php</skin:skin_name>
            <skin:skin_folder>hw_awc_skins</skin:skin_folder>
            <skin:group></skin:group>
            <skin:enable_external_callback></skin:enable_external_callback>

            <skin:allows_skin_names>
                <param>hwawc-skin.php</param>
            </skin:allows_skin_names>
            <skin:properties>
                <params name="migrate_data"></params>
            </skin:properties>
            <skin:list>
                <param name="skin-1"/>
                <param name="skin-2"/>
                <param name="skin-3"/>
            </skin:list>
        </hw:skin>
        <hw:skin name="livechat">
            <skin:apply_plugin>hw-livechat</skin:apply_plugin>
            <skin:default_skin_path>skins</skin:default_skin_path>
            <skin:default_skin>default/</skin:default_skin>
            <skin:skin_type>file</skin:skin_type>
            <skin:skin_name>hw-livechat-skin.php</skin:skin_name>
            <skin:other_folder>hw_livechat_skins</skin:other_folder>
            <skin:enable_external_callback></skin:enable_external_callback>

        </hw:skin>
    </skins>

    <!-- options -->
    <options>
        <option>
            <params name="HW_Sidebar_Settings" prefix="sidebar_1_" method="append">
                <param name="enable_override_sidebar" type="bool">1</param>
                <param name="alphabe_widgets" type="bool">1</param>
                <param name="bgcolor_title"></param>
                <param name="bgimg_title"></param>
                <param name="bgcolor_box"></param>
                <param name="bgimg_box"></param>

                <!-- default skin -->
                <params name="skin_default">
                    <params:skin_encoded name="hash_skin" instance="awc">
                        <skin:skin_type>file</skin:skin_type>
                        <skin:skin>default</skin:skin>
                        <skin:source>plugin</skin:source>
                    </params:skin_encoded>
                    <params:skin_instance name="hwskin_config" instance="awc">

                    </params:skin_instance>
                    <param name="hwskin_condition"><![CDATA[]]></param>
                    <params name="skin_options">
                        <param name="enqueue_css_position" value="head"/>
                        <param name="enqueue_js_position" value="footer"/>

                    </params>
                </params>
                <!-- skin1 -->
                <params name="skin1">

