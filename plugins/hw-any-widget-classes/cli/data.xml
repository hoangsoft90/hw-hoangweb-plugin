    <posts>
        <!-- dynamic sidebar -->
        <item update="1">
            <title>sidebar 1</title>
            <description></description>
            <content:encoded><![CDATA[]]></content:encoded>
            <excerpt:encoded><![CDATA[]]></excerpt:encoded>
